<% project_rendering_image = project.project_rendering_images.new %>
<%= form_with(scope: "image", url: project_project_rendering_images_path(project, project_rendering_image), method: 'post', html: { data: { maxfilesize: CertificationPathDocument::MAXIMUM_DOCUMENT_FILE_SIZE } }) do |f| %>
  <%= f.file_field :rendering_image, accept: ImageUploader.new.extension_white_list_js, required: true %>
  <%= f.submit "Upload", style: "margin: 2px 0 2px 0;" %>
<% end %>
