$('#projects-chart-div').html("<%= escape_javascript(render partial: 'projects_chart', locals: { projects: @projects }) %>");
$('#projects-chart-div').modal('show');


$(function(){
  // call the function
  projects = <%= @projects.to_json.html_safe %>

  function loadFile() {
    return trimNonValid(projects, "2023-01-01")
  }

  function addProjects(){
    projects = loadFile();
    prepareInputBox();
  }

  addProjects()
  drawTables();
});
