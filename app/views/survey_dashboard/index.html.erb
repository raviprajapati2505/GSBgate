<%# <div class="row"> %>
  <div class="col-md-4 col-sm-12 col-xs-12 card">
    <div class="ibox">
      <div class="ibox-title">
        <div class='d-flex justify-content-between align-items-center'>
          <h5><b><%= t('.survey_types.title') %></b></h5>
        </div>
      </div>
      <div class="ibox-content">
        <table class="table table-bordered table-striped">
          <tbody>
            <tr>
              <th><%= t('.survey_types.total_count') %></th>
              <td><%= @survey_types_count %></td>
            </tr>
          </tbody>
        </table>

        <%= btn_link_to(survey_types_path, icon: 'list', text: 'All Survey Types') %>
      </div>
    </div>
  </div>
  <div class="col-md-4 col-sm-12 col-xs-12 card">
    <div class="ibox">
      <div class="ibox-title">
        <div class='d-flex justify-content-between align-items-center'>
