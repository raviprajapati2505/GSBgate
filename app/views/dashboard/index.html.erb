<% else %>
  <%= render partial: 'for_cgp_user', locals: { 
    overdue_licences: @overdue_licences, 
    user_licences_design: @user_licences_design, 
    user_licences_construction: @user_licences_construction, 
    user_licences_operation: @user_licences_operation, 
