<div class="col-lg-12">
  <% survey_question.question_options.each do |option| %>
    <%= question_response.check_box :value, { label: option.option_text, class: 'survey-checkboxes', required: survey_question.mandatory ? true : false, data: { checkbox_value: option.option_text}}, option.option_text, false %>
  <% end %>
</div>