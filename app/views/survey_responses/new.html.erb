<div class="ibox float-e-margins">
    <div class="ibox-content">
      <h2><%= t('survey_response.new.title' ) %></h2>
        <%= bootstrap_form_for(@survey_response, url: submit_survey_responses_path(@project_survey), method: 'post') do |f| %>
            <div class="form-inputs">
              <div class="row">
                  <div class="col-lg-12">
                    <%= f.text_field :name %>
                  </div>
                  <div class="col-lg-12">
                    <%= f.text_field :email %>
                  </div>
                  <div class="col-lg-12">
                      <%= btn_save %>
                  </div>
                </div>
            </div>
        <% end %>
  </div>
</div>